<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.renren.shopping.model">
	<!-- 动态插入和更新，当值为null的时候不加入到sql语句中 -->
	<class name="Travel" table="travels" dynamic-insert="true">
		<id name="id">
			<generator class="native" />
		</id>
		<property name="userId" column="user_id" type="int" not-null="true" />
		<property name="toCountry" column="to_country" type="string"
			not-null="true" />
		<property name="resident" type="int" />
		<property name="departureCity" column="departure_city"
			length="64" />
		<property name="back_time" type="long" not-null="true" />
		<property name="start_time" type="long" not-null="true" />
		<property name="description" type="string" length="256" />
		<property name="addreddOtherId" column="address_id" type="int"
			insert="false" update="false" />
		<property name="delete_flag" type="int" />
		<property name="update_time" type="timestamp" />
	</class>
</hibernate-mapping>