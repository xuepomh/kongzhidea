<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.renren.shopping.model">
	<!-- 动态插入和更新，当值为null的时候不加入到sql语句中 -->
	<class name="AddressOther" table="address_other" dynamic-insert="true">
		<id name="id">
			<generator class="native" />
		</id>
		<property name="userId" column="user_id" not-null="true"
			type="int" />
		<property name="receiverName" column="receiver_name" type="string"
			length="32" not-null="true" />
		<property name="countryId" column="country_id" type="int"
			insert="false" update="false" />
		<property name="addressDetail" column="address_detail" type="string"
			length="1024" not-null="true" />
		<property name="zipcode" column="zipcode" type="string"
			length="11" />
		<property name="tel" column="tel" type="string" length="11"
			not-null="true" />
		<property name="isDefault" column="is_default" type="int" />
		<property name="update_time" type="timestamp" />
	</class>
</hibernate-mapping>